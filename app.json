{
  "expo": {
    "name": "HowUDoin",
    "slug": "HowUDoin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios.png",
    "scheme": "howudoin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D3C6BA"
    },
    
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Adaptive.png",
        "backgroundColor": "#000000"
      }
    },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ios.png"
    },
    
    "plugins": 
    [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}